<xml>
  <map name="PACKET_SIZES">
    <pair key='INT8' value="1"></pair>
    <pair key='UINT8' value="1"></pair>
    <pair key='INT16' value="2"></pair>
    <pair key='UINT16' value="2"></pair>
    <pair key='INT32' value="4"></pair>
    <pair key='UINT32' value="4"></pair>
    <pair key='INT64' value="8"></pair>
    <pair key='UINT64' value="8"></pair>

    <pair key='TAG_POSITION' value="12"></pair>
    <pair key='FIELD_POSITION' value="6"></pair>
    <pair key='OBSTACLE' value="16"></pair>
    <pair key='MOTOR_INFO' value="16"></pair>
    <pair key='BITARRAY8' value="1"></pair>
    <pair key='ROBOT_STATE' value="1"></pair>
    <pair key='NAVIGATION_PATH' value="200"></pair>
    <pair key='NAVIGATION_OBSTACLES' value="200"></pair>
    <pair key='NAVIGATION_POINT' value="4"></pair>
    <pair key='NAVIGATION_OBSTACLES' value="120"></pair>
    <pair key='NAVIGATION_OBSTACLE' value="8"></pair>
    <pair key='SLAM_STATE' value="120"></pair>
    <pair key='HARDWARE' value="8"></pair>
    <pair key='JOYSTICK' value="8"></pair>
    <pair key='VISION' value="31"></pair>
    <pair key='REALSENSE' value="127"></pair>
    <pair key='DATA_SERVER' value="127"></pair>
    <pair key='DASHBOARD' value="9"></pair>
    <pair key='SENSOR_STATE' value="160"></pair>
    <pair key='VISION_COMMAND' value="127"></pair>
    <pair key='REALSENSE_COMMAND' value="127"></pair>
    <pair key='VISION_IMAGE' value="8191"></pair>
    <pair key='IDENTIFIER' value="127"></pair>
    <pair key='VISION_PROPERTIES' value="127"></pair>
    <pair key='DEBUG_MESSAGE' value="129"></pair>
  </map>

  <enum name="TYPES">
    <element name="JOYSTICK" value="1"></element>
    <element name="VISION" value="2"></element>
    <element name="REALSENSE" value="3"></element>
    <element name="SENSOR_STATE" value="10"></element>
    <element name="DATA_SERVER" value="8"></element>
    <element name="DASHBOARD" value="9"></element>
    <element name="IDENTIFIER" value="250"></element>
    <element name="ROBOT_STATE" value="11"></element>
    <element name="VISION_COMMAND" value="12"></element>
    <element name="VISION_IMAGE" value="13"></element>
    <element name="VISION_PROPERTIES" value="14"></element>
    <element name="HARDWARE" value="15"></element>
    <element name="REALSENSE_COMMAND" value="16"></element>
    <element name="NAVIGATION_PATH" value="17"></element>
    <element name="SLAM_STATE" value="18"></element>
    <element name="NAVIGATION_OBSTACLES" value="19"></element>
    <element name="DEBUG_MESSAGE" value="20"></element>
    <element name="INVALID" value="255"></element>
  </enum>
  <enum name="IDENTIFIERS">
    <element name="DASHBOARD" value="1"></element>
    <element name="VISION" value="2"></element>
    <element name="TCPSERIAL" value="3"></element>
    <element name="HARDWARE" value="4"></element>
    <element name="DATASAVER" value="5"></element>
    <element name="REALSENSE" value="6"></element>
    <element name="SLAM" value="7"></element>
    <element name="NAVIGATION" value="8"></element>
  </enum>

  <enum name="VISION_COMMAND">
    <element name="send_image" value="0"></element>
    <element name="start_streaming" value="1"></element>
    <element name="stop_streaming" value="2"></element>
    <element name="start_detection" value="3"></element>
    <element name="stop_detection" value="4"></element>

  </enum>

  <message name="Joystick">
    <field name="buttons_1" type="BitArray8">
      <accessor type="bits">
        <bit>button_A</bit>
        <bit>button_B</bit>
        <bit>button_X</bit>
        <bit>button_Y</bit>
        <bit>button_LB</bit>
        <bit>button_RB</bit>
        <bit>button_Select</bit>
        <bit>button_Start</bit>
      </accessor>
    </field>
    <field name="buttons_2" type="BitArray8">
      <accessor type="bits">
        <bit>button_LJ</bit>
        <bit>button_RJ</bit>
        <bit>button_POVU</bit>
        <bit>button_POVR</bit>
        <bit>button_POVD</bit>
        <bit>button_POVL</bit>
      </accessor>
    </field>

    <field name="lj_x" type="int8">
      <accessor type="float" scale="0.00787401574"></accessor>
    </field>
    <field name="lj_y" type="int8">
      <accessor type="float" scale="0.00787401574"></accessor>
    </field>
    <field name="rj_x" type="int8">
      <accessor type="float" scale="0.00787401574"></accessor>
    </field>
    <field name="rj_y" type="int8">
      <accessor type="float" scale="0.00787401574"></accessor>
    </field>
    <field name="lt" type="int8">
      <accessor type="float" scale="0.00787401574"></accessor>
    </field>
    <field name="rt" type="int8">
      <accessor type="float" scale="0.00787401574"></accessor>
    </field>
  </message>

  <message name="Tag_Position">
    <field name="yaw" type="int16">
      <accessor type="float" scale="0.0027465827"></accessor>
    </field>
    <field name="pitch" type="int16">
      <accessor type="float" scale="0.0027465827"></accessor>
    </field>
    <field name="roll" type="int16">
      <accessor type="float" scale="0.0027465827"></accessor>
    </field>
    <field name="x" type="int16">
      <accessor type="float" scale="0.1"></accessor>
    </field>
    <field name="y" type="int16">
      <accessor type="float" scale="0.1"></accessor>
    </field>
    <field name="z" type="int16">
      <accessor type="float" scale="0.1"></accessor>
    </field>
  </message>

  <message name="Field_Position">
    <field name="yaw" type="int16">
      <accessor type="float" scale="0.0027465827"></accessor>
    </field>
    <field name="x" type="int16">
      <accessor type="float" scale="0.1"></accessor>
    </field>
    <field name="y" type="int16">
      <accessor type="float" scale="0.1"></accessor>
    </field>
  </message>

  <message name="Obstacle">
    <field name="x" type="int16">
      <accessor type="float" scale="0.1"></accessor>
    </field>
    <field name="y" type="int16">
      <accessor type="float" scale="0.1"></accessor>
    </field>
    <field name="width" type="int16">
      <accessor type="float" scale="0.1"></accessor>
    </field>
    <field name="height" type="int16">
      <accessor type="float" scale="0.1"></accessor>
    </field>
  </message>

  <message name="Navigation_Point">
    <field name="x" type="int16">
    </field>
    <field name="y" type="int16">
    </field>
  </message>

  <message name="Navigation_Path">
    <field name="point_0" type="Navigation_Point"></field>
    <field name="point_1" type="Navigation_Point"></field>
    <field name="point_2" type="Navigation_Point"></field>
    <field name="point_3" type="Navigation_Point"></field>
    <field name="point_4" type="Navigation_Point"></field>
    <field name="point_5" type="Navigation_Point"></field>
    <field name="point_6" type="Navigation_Point"></field>
    <field name="point_7" type="Navigation_Point"></field>
    <field name="point_8" type="Navigation_Point"></field>
    <field name="point_9" type="Navigation_Point"></field>
    <field name="point_10" type="Navigation_Point"></field>
    <field name="point_11" type="Navigation_Point"></field>
    <field name="point_12" type="Navigation_Point"></field>
    <field name="point_13" type="Navigation_Point"></field>
    <field name="point_14" type="Navigation_Point"></field>
    <field name="point_15" type="Navigation_Point"></field>
    <field name="point_16" type="Navigation_Point"></field>
    <field name="point_17" type="Navigation_Point"></field>
    <field name="point_18" type="Navigation_Point"></field>
    <field name="point_19" type="Navigation_Point"></field>
    <field name="point_20" type="Navigation_Point"></field>
  </message>

  <message name="Navigation_Obstacle">
    <field name="x" type="int16">
    </field>
    <field name="y" type="int16">
    </field>
    <field name="width" type="int16">
    </field>
    <field name="height" type="int16">
    </field>
  </message>

  <message name="Navigation_Obstacles">
    <field name="obstacle_0" type="Navigation_Obstacle"></field>
    <field name="obstacle_1" type="Navigation_Obstacle"></field>
    <field name="obstacle_2" type="Navigation_Obstacle"></field>
    <field name="obstacle_3" type="Navigation_Obstacle"></field>
  </message>

  <message name="Slam_State">
    <field name="field_position" type="Field_Position"></field>
  </message>


  <message name="Vision">
    <field name='tag0' type='Tag_Position'></field>
    <field name='tag1' type='Tag_Position'></field>
    <field name='field_position' type='Field_Position'></field>
    <field name='vision_good' type='uint8'></field>
  </message>

  <message name="Vision_Command">
    <field name='command' type='uint8'></field>
  </message>

  <message name="Realsense_Command">
    <field name='command' type='uint8'></field>
  </message>

  <message name="Vision_Properties">
    <field name='exposure' type='uint32'></field>
    <field name='gain' type='uint32'></field>
  </message>

  <message name="Realsense">
    <field name='obstacle_1' type='Obstacle'></field>
    <field name='obstacle_2' type='Obstacle'></field>
    <field name='obstacle_3' type='Obstacle'></field>
    <field name='obstacle_4' type='Obstacle'></field>
  </message>

  <message name="Identifier">
    <field name='identifier' type='uint8'></field>
  </message>

  <message name="Motor_Info">
    <field name='can_id' type='uint8'></field>
    <field name='current' type='int16'>
      <accessor type="float" scale="100.0"></accessor>
    </field>
    <field name='position' type='int64'>
      <accessor type="float" scale="1/150.0"></accessor>
    </field>
    <field name='velocity' type='int32'>
      <accessor type="float" scale="0.1"></accessor>
    </field>
    <field name='percent' type='int8'></field>
  </message>

  <message name="Robot_State">
    <field name='state' type='uint8'></field>
  </message>

  <message name="Sensor_State">
    <field name='motor_info_1' type='Motor_Info'></field>
    <field name='motor_info_2' type='Motor_Info'></field>
    <field name='motor_info_3' type='Motor_Info'></field>
    <field name='motor_info_4' type='Motor_Info'></field>
    <field name='motor_info_5' type='Motor_Info'></field>
    <field name='motor_info_6' type='Motor_Info'></field>
    <field name='motor_info_7' type='Motor_Info'></field>
    <field name='motor_info_8' type='Motor_Info'></field>
    <field name='motor_info_9' type='Motor_Info'></field>
    <field name='motor_info_10' type='Motor_Info'></field>
  </message>

  <message name="Data_Server">
    <field name='connected_status' type='BitArray8'>
      <accessor type="bits">
        <bit>hero_connected </bit>
        <bit>vision_connected </bit>
        <bit>dashboard_connected </bit>
        <bit>realsense_connected </bit>
        <bit>tcpserial_connected</bit>
      </accessor>
    </field>
  </message>

  <message name="Dashboard">
    <field name='enabled_1' type='uint8'></field>
    <field name='enabled_2' type='uint8'></field>
    <field name='enabled_3' type='uint8'></field>
    <field name='enabled_4' type='uint8'></field>
    <field name='enabled_5' type='uint8'></field>
    <field name='enabled_6' type='uint8'></field>
    <field name='enabled_7' type='uint8'></field>
    <field name='enabled_8' type='uint8'></field>
    <field name='state' type='uint8'></field>
  </message>

  <message name="Hardware">
    <field name='angle' type='uint8'></field>
    <field name='r' type='uint8'></field>
    <field name='g' type='uint8'></field>
    <field name='b' type='uint8'></field>
  </message>

  <message name="Vision_Image">
    <field name='image' type='bytearray' size='8191'></field>
  </message>

  <message name="Debug_Message">
    <field name='identifier' type='uint8'></field>
    <field name='message' type='bytearray' size='128'></field>
  </message>

</xml>